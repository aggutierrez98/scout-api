{
  "name": "scout-api",
  "version": "1.0.0",
  "description": "Scout api for personal use",
  "main": "index.js",
  "scripts": {
    "test": "npm run test",
    "build": "npx tsc",
    "start": "dotenv -e .env.production -- node dist/index.js",
    "dev": "dotenv -e .env.production -- ts-node-dev src/index.ts",
    "import-scouts:dev": "dotenv -e .env.development -- ts-node src/bin/updateScouts.ts",
    "import-scouts": "dotenv -e .env.production -- ts-node src/bin/updateScouts.ts",
    "push:dev": "dotenv -e .env.development -- prisma db push",
    "studio:dev": "dotenv -e .env.development -- prisma studio",
    "studio": "dotenv -e .env.production -- prisma studio"
  },
  "author": "aggutierrez",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.3.0",
    "esm": "^3.2.25",
    "execa": "^8.0.1",
    "express": "4.18.2",
    "http-terminator": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "3.6.0",
    "node-cron": "3.0.2",
    "progress": "^2.0.3",
    "qrcode-terminal": "0.12.0",
    "whatsapp-web.js": "1.22.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.2",
    "@types/bcryptjs": "^2.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "20.5.0",
    "@types/progress": "^2.0.5",
    "@types/qrcode-terminal": "0.12.0",
    "nodemon": "3.0.1",
    "prisma": "5.1.1",
    "rome": "12.1.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aggutierrez98/scout-api.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/aggutierrez98/scout-api/issues"
  },
